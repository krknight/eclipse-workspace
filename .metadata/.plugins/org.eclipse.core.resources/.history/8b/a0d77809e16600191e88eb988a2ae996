#include <iostream>
#include <list>
using namespace std;

void showlist (list <int> g) {
	list <int> :: iterator it;  // this is how to declare an iterator, vector<int>::iterator ptr;
	for(it = g.begin(); it != g.end(); ++it) {
		cout << '\t' << *it;
	}
	cout << '\n';
}

int main() {
	list <int> gqlist1, gqlist2;

	for (int i = 0; i < 10; i++) {
		gqlist1.push_back(i*2);
		gqlist2.push_front(i*3);
	}

	cout << "gqlist1 is ";
	showlist(gqlist1);
	cout << "\ngqlist2 is ";
	showlist(gqlist2);
	return 0;
}
