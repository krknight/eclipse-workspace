#include <iostream>
#include "vehicle.h"
#include "Tesla.h"
#include "RangeRover.h"

using namespace std;

int main() {

	vehicle v;
	Tesla t;
	RangeRover r;
	v.propulsion();
	t.propulsion();
	r.propulsion();
	return 0;
}
