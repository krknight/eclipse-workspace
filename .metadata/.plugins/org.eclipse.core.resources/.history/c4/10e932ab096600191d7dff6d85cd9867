#ifndef VEHICLE_H_
#define VEHICLE_H_


class vehicle {
private: int base;

vehicle();
~vehicle();

public:
	void virtual propulsion();
};

class RangeRover : public vehicle {
	RangeRover();
	~RangeRover();
};

#endif /* VEHICLE_H_ */
