#include <iostream>
#include <vector>

using namespace std;

int main() {
	vector<int> vptr(){0,1,2,3,4};

//	vptr.push_back(1);
//	vptr.push_back(2);
//	vptr.push_back(3);
//	vptr.pop_back();

	for (int i=0;i<vptr.size();i++) {
		cout << vptr[i] << endl;
	}

	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
