#include <iostream>
#include "vehicle.h"
#include "Tesla.h"
#include "RangeRover.h"

using namespace std;

int main() {

	vehicle v;
	Tesla t;
	RangeRover r;
	cout << v.propulsion() << endl;
	cout << t.propulsion() << endl;
	cout << r.propulsion() << endl;
	return 0;
}
